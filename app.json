{
  "expo": {
    "name": "Twitter Clone",
    "description": "Programming Laboratory Academic project",
    "slug": "twitter-clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/twitter-icon-circle.png",
    "splash": {
      "image": "./assets/icons/twitter-icon-circle.png",
      "resizeMode": "contain",
      "backgroundColor": "#1da1f2"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/twitter-icon-circle.png",
        "backgroundColor": "#1da1f2"
      }
    },
    "web": {
      "favicon": "./assets/icons/twitter-icon-circle.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-ads-admob",
        {
          "userTrackingPermission": "This identifier will be used to deliver personalized ads to you."
        }
      ]
    ]
  }
}
